{
  "props":{
    "mapstruct":false,
    "lombok":true,
    "rootPackage":"com.example.demo",
    "package":"domain",
    "repositories":{
      "generate":true,
      "all":false,
      "package":"repositories"
    },
    "spaces":4
  },
  "entities":{
    "Role":{
      "repository":true,
      "primaryKey":"id",
      "attributes":{
        "id":"long",
        "name":"string"
      }
    },
    "Permission":{
      "repository":true,
      "primaryKey":"id",
      "attributes":{
        "id":"long",
        "role":"role",
        "canRead":"boolean",
        "canInsert":"boolean",
        "canUpdate":"boolean",
        "canDelete":"boolean"
      }
    },
    "User":{
      "repository":true,
      "primaryKey":"id",
      "attributes":{
        "id":"long",
        "username":"string",
        "password":"string",
        "firstname":"string",
        "lastname":"string",
        "email":"string",
        "phone":"string"
      }
    }
  }
}